{
  "name": "more-recipes-personal",
  "version": "1.0.0",
  "description": "A Recipe Management Application written in Javscript (ES6)",
  "author": "Iveren Shaguy",
  "license": "ISC",
  "main": "server/build/bin/www.js",
  "scripts": {
    "start": "npm run start:server",
    "start:dev": "npm-run-all --parallel start:server:dev start:client",
    "build": "npm-run-all build:server build:client",
    "test": "npm-run-all test:server test:client",
    "test:e2e": "cross-env NODE_ENV=test nightwatch",
    "e2e:setup": "selenium-standalone install --config=./selenium.config.js",
    "start:server": "node ./server/build/bin/www.js",
    "start:server:dev": "cross-env NODE_ENV=development nodemon --exec babel-node ./server/build/bin/www.js",
    "start:client": "node client/scripts/start.js",
    "build:server": "cross-env NODE_ENV=development babel server/src -d server/build",
    "build:client": "node client/scripts/build.js",
    "migrate": "sequelize db:migrate:undo:all && sequelize db:migrate",
    "migrate:test": "sequelize db:migrate:undo:all --env=test && sequelize db:migrate --env=test",
    "seed": "sequelize db:seed:all",
    "seed:test": "sequelize db:seed:all --env=test",
    "test:server": "cross-env NODE_ENV=test SECRET=mytestsecret nyc mocha server/tests/test.js --recursive --timeout 30000",
    "test:server:watch": "cross-env NODE_ENV=test SECRET=mytestsecret mocha server/tests/test.js --recursive --timeout 30000 --watch",
    "test:client": "node client/scripts/test.js --env=jsdom --coverage",
    "test:client:watch": "node client/scripts/test.js --env=jsdom",
    "pretest": "npm run lint",
    "pretest:server": "npm-run-all migrate:test seed:test",
    "pretest:server:watch": "npm-run-all migrate:test seed:test",
    "pretest:client": "npm run lint",
    "posttest": "lcov-result-merger ./*/coverage/lcov.info",
    "prebuild:init": "cd server && mkdir build",
    "prebuild": "npm-run-all clean prebuild:init",
    "clean": "npm-run-all clean:server clean:client",
    "clean:server": "cd server && rm -rf build",
    "clean:client": "cd client && rm -rf build",
    "lint": "eslint **/*.js --fix",
    "docs": "aglio -i docs.apib --theme-variables flatly --theme-template triple -o docs/index.html",
    "coverage": "lcov-result-merger ./*/coverage/lcov.info | coveralls",
    "analyze": "source-map-explorer client/build/static/js/main.*"
  },
